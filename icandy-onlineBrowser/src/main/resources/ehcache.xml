<ehcache name="shiro" updateCheck="false" monitoring="autodetect"
	dynamicConfig="true">

	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="360" timeToLiveSeconds="360" overflowToDisk="false"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />

	<cache name="defaultCache" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="1200" timeToLiveSeconds="1200" overflowToDisk="false"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />
	<cache name="T_HOST" maxElementsInMemory="10000"
		maxElementsOnDisk="10000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="0" diskPersistent="false" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU">
	</cache>
	<cache name="T_USER" maxElementsInMemory="10000"
		maxElementsOnDisk="10000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="0" diskPersistent="false" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU">
	</cache>
	<cache name="T_SEND_RESULT" maxElementsInMemory="100000"
		maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="0" diskPersistent="false" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU">
	</cache>
	<cache name="T_LOTTERY" maxElementsInMemory="100000"
		maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="0" diskPersistent="false" timeToLiveSeconds="600000"
		memoryStoreEvictionPolicy="LFU">
	</cache>
	<cache name="QIHAO_MAP_CACHE" maxElementsInMemory="100000"
		maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="0" diskPersistent="false" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU">
	</cache>
</ehcache>